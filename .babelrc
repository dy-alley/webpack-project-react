{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "usage", 
            "corejs": 2, 
            "targets":{
                "browsers":[
                    "last 2 versions"
                ]
            }
        }],
        ["@babel/preset-react"]
    ],
    "plugins":[
        "@babel/plugin-transform-runtime",
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "@babel/plugin-syntax-dynamic-import"
    ]
}